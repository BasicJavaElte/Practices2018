import java.util.Formatter;

public class MidNight{
	public static int fib (int n){
			if(n==0){
			return 0;}
			if(n==1){
			return 1;}
			return (fib(n-1)+fib(n-2));
		}
	public static void main (String[] args){
		
		
		for(int i=0;i<40;i++)
		{
			int aux=fib(i);
			System.out.printf("%s%d%-3s%d%n","fib(",i,"):",aux);
		}
		
	}
	
	
}
